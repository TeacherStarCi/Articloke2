    <servlet>
        <servlet-name>ResetParamsServlet</servlet-name>
        <servlet-class>Controllers.SearchArticle.ResetParamsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignOutServlet</servlet-name>
        <servlet-class>Controllers.Authenication.SignOutServlet</servlet-class>
