<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SignInServlet</servlet-name>
        <servlet-class>Controllers.Authenication.SignInServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GoogleSignInServlet</servlet-name>
        <servlet-class>Controllers.Authenication.GoogleSignIn.GoogleSignInServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpServlet</servlet-name>
        <servlet-class>Controllers.Authenication.SignUp.SignUpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InitServerServlet</servlet-name>
        <servlet-class>Controllers.Init.InitServerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowArticleResultServlet</servlet-name>
        <servlet-class>Controllers.SearchArticle.ShowArticleResultServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MovePageServlet</servlet-name>
        <servlet-class>Controllers.SearchArticle.MovePageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResetParamsServlet</servlet-name>
        <servlet-class>Controllers.SearchArticle.ResetParamsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignOutServlet</servlet-name>
        <servlet-class>Controllers.Authenication.SignOutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TransferFromHomeToSearchServlet</servlet-name>
        <servlet-class>Controllers.SearchArticle.TransferFromHomeToSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignInServlet</servlet-name>
        <url-pattern>/SignInServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GoogleSignInServlet</servlet-name>
        <url-pattern>/GoogleSignInServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
        <url-pattern>/SignUpServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InitServerServlet</servlet-name>
        <url-pattern>/InitServerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowArticleResultServlet</servlet-name>
        <url-pattern>/ShowArticleResultServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MovePageServlet</servlet-name>
        <url-pattern>/MovePageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResetParamsServlet</servlet-name>
        <url-pattern>/ResetParamsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignOutServlet</servlet-name>
        <url-pattern>/SignOutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TransferFromHomeToSearchServlet</servlet-name>
        <url-pattern>/TransferFromHomeToSearchServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file> InitServerServlet </welcome-file>
    </welcome-file-list>
</web-app>
